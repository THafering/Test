<script type="text/javascript" charset="utf-8">
    jQuery(document).ready(function() {						
        function addMega(){
            jQuery(this).addClass("hovering");
        }
                
        function removeMega(){
            jQuery(this).removeClass("hovering");
        }
                
        var megaConfig = {
            interval: 250,
            sensitivity: 4,
            over: addMega,
            timeout: 500,
            out: removeMega
        };
                
        jQuery("li.mega").hoverIntent(megaConfig)
    
        });
</script>
